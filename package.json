    "mocha": "^3.1.1",
    "mocha-lcov-reporter": "^1.1.0",
    "pem": "^1.8.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.5",
    "sinon": "^1.17.5",
    "standard": "^8.5.0",
